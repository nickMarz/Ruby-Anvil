# Anvil API Configuration
# Copy this file to .env and add your actual API keys

# Your Anvil API key (get it from https://app.useanvil.com/organizations/settings/api)
ANVIL_API_KEY=your_dev_api_key_here

# Optional: Webhook token for verifying webhooks
ANVIL_WEBHOOK_TOKEN=your_webhook_token_here

# Optional: Environment (development or production)
ANVIL_ENV=development